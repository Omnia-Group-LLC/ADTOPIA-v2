    "test:debug": "vitest run src/lib/agents/__tests__/debug.test.ts",
    "test:hybrid": "vitest run src/lib/agents/__tests__/debug.test.ts -- --grep hybrid",
    "test:studio": "vitest run src/lib/agents/__tests__/studio-autogen.test.ts",
    "test:state-ab": "vitest run src/lib/agents/__tests__/studio-autogen.test.ts -- --grep 'State A/B'",
