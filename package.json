    "test:agents": "./scripts/test-agent-system.sh",
    "git-repo:setup": "./scripts/setup-git-repo.sh",
    "git-repo:config": "git-repo config wizard",
    "generate:variants": "tsx -e \"import('./src/lib/agents/variant-generator.ts').then(m => m.generateVariants({source: process.argv[2] || 'rmovers/modesto', target: process.argv[3] || 'AdTopia gallery upsell', count: parseInt(process.argv[4]) || 5}).then(v => console.log(JSON.stringify(v, null, 2))))\"",
    "agent:create": "tsx scripts/create-claude-agent.ts",
    "agent:list": "tsx scripts/agent-manager.ts list",
    "agent:execute": "tsx scripts/agent-manager.ts execute",
    "evolution:report": "node scripts/generate-evolution-report.js",
    "evolution:suggestions": "tsx -e \"import('./src/lib/evolution/upgrade-tracker.js').then(m => m.default.getAllUpgrades().then(s => console.log(JSON.stringify(s, null, 2))))\"",
    "evolution:delegate": "tsx -e \"import('./src/lib/evolution/weekly-report-generator.js').then(m => m.default.generateWeeklyReport({delegate: true}).then(r => console.log(JSON.stringify(r, null, 2))))\"",
    "research:global": "tsx -e \"import('./src/lib/agents/research-agent.js').then(m => m.default.performResearch({query: process.argv[2] || 'AI agent architecture', language: process.argv[3] || 'en'}).then(r => console.log(JSON.stringify(r, null, 2))))\"",
    "delegate:test": "tsx -e \"import('./src/lib/agents/delegator.js').then(m => m.delegate({type: process.argv[2] || 'creative_variant', prompt: process.argv[3] || 'Test delegation'}).then(r => console.log(JSON.stringify(r, null, 2))))\"",
    "manus:scrape": "tsx -e \"import('./src/lib/utils/manus-scraper.js').then(m => m.scrapeWithManus({query: process.argv[2] || 'Facebook ads', sources: ['facebook', 'instagram']}).then(r => console.log(JSON.stringify(r, null, 2))))\"",
    "scout:mock": "tsx -e \"import('./src/lib/utils/manus-scraper.js').then(m => m.generateScoutMock(process.argv[2] || 'Bento grid mock', parseInt(process.argv[3]) || 60).then(r => console.log(JSON.stringify(r, null, 2))))\""
