  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose",
    "test:mem0": "vitest run src/lib/agents/__tests__/mem0.test.ts",
    "test:langgraph": "vitest run src/lib/agents/__tests__/langgraph.test.ts",
    "test:vision": "vitest run src/lib/agents/__tests__/vision.test.ts",
    "test:dash": "vitest run src/lib/dash/__tests__/realtime.test.ts",
    "test:prd": "vitest run src/lib/api/__tests__/prd-sync.test.ts",
